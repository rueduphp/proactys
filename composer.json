{
    "name": "proactys/website",
    "description": "proactys website.",
    "keywords": ["proactys"],
    "type": "project",
    "repositories": [
      {
        "type": "path",
        "url": "./nova"
      }
    ],
    "require": {
        "php": "^7.1.3",
        "cakephp/chronos": "~1.0",
        "coderello/laravel-nova-lang": "^0.1.20",
        "digitalcloud/multilingual-nova": "^1.0",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "^4.0",
        "fzaninotto/faker": "^1.4",
        "http-interop/http-middleware": "^0.4",
        "intervention/image": "*",
        "intervention/imagecache": "*",
        "laravel/framework": "5.7.*",
        "laravel/nova": "*",
        "laravel/tinker": "^1.0",
        "moontoast/math": "^1.1",
        "orlyapps/nova-belongsto-depend": "^0.0.6",
        "predis/predis": "^1.1.1",
        "rueduphp/octo": "2.1.*",
        "silber/bouncer": "*",
        "spatie/once": "^1.1",
        "symfony/finder": "~4.0",
        "symfony/yaml": "*",
        "twig/twig": "~1.30",
        "vyuldashev/nova-permission": "^1.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "files": [
          "app/helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "Proactys\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
